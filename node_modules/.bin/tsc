#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/bin/node_modules:/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/node_modules:/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/typescript@5.8.3/node_modules:/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/bin/node_modules:/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/typescript@5.8.3/node_modules/typescript/node_modules:/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/typescript@5.8.3/node_modules:/run/media/phantom/7a980b55-37e7-4c97-a092-e27ee8dfa6a3/@home/phantom/coding/cohort2-test/ts-web/ts-web-trial/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typescript@5.8.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../.pnpm/typescript@5.8.3/node_modules/typescript/bin/tsc" "$@"
fi
